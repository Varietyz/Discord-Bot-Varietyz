        "@sapphire/discord.js-utilities": "^7.3.3",
        "@sapphire/framework": "^5.3.4",
        "@wise-old-man/utils": "^3.3.10",
        "async-mutex": "^0.5.0",
        "axios": "^1.9.0",
        "canvas": "^3.1.0",
        "cli-progress": "^3.12.0",
        "cors": "^2.8.5",
        "csv-parser": "^3.2.0",
        "csv-writer": "^1.6.0",
        "deasync": "^0.1.30",
        "discord-api-types": "^0.38.4",
        "discord.js": "^14.19.3",
        "dotenv": "^16.5.0",
