{
    "name": "varietyz-bot",
    "version": "1.0.0",
    "main": "src/main.js",
    "scripts": {
        "bot": "node src/main.js",
        "generate-readme": "jsdoc2md --files \"src/**/*.js\" > JSDOC_README.md",
        "test": "echo \"Error: no test specified\" && exit 1",
        "lint-jsdoc": "eslint src/**/*.js",
        "update-readme": "npm run generate-readme",
        "precommit": "npm run lint-jsdoc && npm run update-readme",
        "lint": "eslint src/**/*.js",
        "lint:fix": "eslint src/**/*.js --fix",
        "replace-ephemeral": "node .vscode/scripts/replaceEphemeral",
        "generate:cache": "node src/scripts/populateImageCache.js",
        "path-lint": "eslint . --ignore-path .eslint/.eslintignore --config .eslint/.eslintrc.js",
        "deploy": "node deploy.bot.js",
        "push": "python3 strip_comments.py && npm run lint:fix && npm run deploy"
    },
    "engines": {
        "node": ">=20.0.0"
    },
    "keywords": [],
    "author": "Jay Baleine",
    "license": "ISC",
    "description": "Varietyz Discord Bot dependencies",
    "devDependencies": {
        "docdash": "^2.0.2",
        "eslint": "^9.26.0",
        "eslint-config-prettier": "^10.1.3",
        "eslint-plugin-jsdoc": "^50.6.11",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-prettier": "^5.4.0",
        "globals": "^16.1.0",
        "jsdoc": "^4.0.4",
        "jsdoc-to-markdown": "^9.1.1",
        "minami": "^1.2.3",
        "prettier": "^3.5.3",
        "prettier-eslint": "^16.4.2"
    },
    "dependencies": {
        "@discordjs/builders": "^1.11.2",
